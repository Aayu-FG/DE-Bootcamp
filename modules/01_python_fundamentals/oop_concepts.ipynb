    "## 3. Inheritance"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Base class for data processors\n",
    "class DataProcessor:\n",
    "    \"\"\"\n",
    "    Base class for data processing operations.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, name: str) -> None:\n",
    "        \"\"\"\n",
    "        Initialize the data processor.\n",
    "        \n",
    "        Args:\n",
    "            name: Name of the processor\n",
    "        \"\"\"\n",
    "        self.name = name\n",
    "        self.processed_count = 0\n",
    "    \n",
    "    def process(self, data: List[Any]) -> List[Any]:\n",
    "        \"\"\"\n",
    "        Base process method - to be overridden by subclasses.\n",
    "        \n",
    "        Args:\n",
    "            data: Data to process\n",
    "        \n",
    "        Returns:\n",
    "            Processed data\n",
    "        \"\"\"\n",
    "        self.processed_count += len(data)\n",
    "        return data\n",
    "    \n",
    "    def get_stats(self) -> Dict[str, Any]:\n",
    "        \"\"\"\n",
    "        Get processing statistics.\n",
    "        \n",
    "        Returns:\n",
    "            Dictionary with statistics\n",
    "        \"\"\"\n",
    "        return {\n",
    "            \"name\": self.name,\n",
    "            \"processed_count\": self.processed_count\n",
    "        }\n",
    "\n",
    "# Derived class for number processing\n",
    "class NumberProcessor(DataProcessor):\n",
    "    \"\"\"\n",
    "    Processor for numeric data operations.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, name: str, operation: str = \"square\") -> None:\n",
    "        \"\"\"\n",
    "        Initialize the number processor.\n",
    "        \n",
    "        Args:\n",
    "            name: Name of the processor\n",
    "            operation: Type of operation (\"square\", \"double\", \"abs\")\n",
    "        \"\"\"\n",
    "        super().__init__(name)  # Call parent constructor\n",
    "        self.operation = operation\n",
    "    \n",
    "    def process(self, data: List[float]) -> List[float]:\n",
    "        \"\"\"\n",
    "        Process numeric data based on the specified operation.\n",
    "        \n",
    "        Args:\n",
    "            data: List of numbers to process\n",
    "        \n",
    "        Returns:\n",
    "            Processed numbers\n",
    "        \"\"\"\n",
    "        super().process(data)  # Call parent method for counting\n",
    "        \n",
    "        if self.operation == \"square\":\n",
    "            return [x ** 2 for x in data]\n",
    "        elif self.operation == \"double\":\n",
    "            return [x * 2 for x in data]\n",
    "        elif self.operation == \"abs\":\n",
    "            return [abs(x) for x in data]\n",
    "        else:\n",
    "            return data\n",
    "\n",
    "# Derived class for text processing\n",
    "class TextProcessor(DataProcessor):\n",
    "    \"\"\"\n",
    "    Processor for text data operations.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, name: str, case_transform: str = \"upper\") -> None:\n",
    "        \"\"\"\n",
    "        Initialize the text processor.\n",
    "        \n",
    "        Args:\n",
    "            name: Name of the processor\n",
    "            case_transform: Type of case transformation (\"upper\", \"lower\", \"title\")\n",
    "        \"\"\"\n",
    "        super().__init__(name)\n",
    "        self.case_transform = case_transform\n",
    "    \n",
    "    def process(self, data: List[str]) -> List[str]:\n",
    "        \"\"\"\n",
    "        Process text data based on the specified transformation.\n",
    "        \n",
    "        Args:\n",
    "            data: List of strings to process\n",
    "        \n",
    "        Returns:\n",
    "            Processed strings\n",
    "        \"\"\"\n",
    "        super().process(data)\n",
    "        \n",
    "        if self.case_transform == \"upper\":\n",
    "            return [text.upper() for text in data]\n",
    "        elif self.case_transform == \"lower\":\n",
    "            return [text.lower() for text in data]\n",
    "        elif self.case_transform == \"title\":\n",
    "            return [text.title() for text in data]\n",
    "        else:\n",
    "            return data\n",
    "\n",
    "# Using inheritance\n",
    "numbers = [1, -2, 3, -4, 5]\n",
    "texts = [\"hello\", \"world\", \"python\", \"programming\"]\n",
    "\n",
    "# Create processors\n",
    "square_processor = NumberProcessor(\"Square Processor\", \"square\")\n",
    "abs_processor = NumberProcessor(\"Absolute Processor\", \"abs\")\n",
    "upper_processor = TextProcessor(\"Upper Case Processor\", \"upper\")\n",
    "\n",
    "# Process data\n",
    "squared_numbers = square_processor.process(numbers)\n",
    "absolute_numbers = abs_processor.process(numbers)\n",
    "upper_texts = upper_processor.process(texts)\n",
    "\n",
    "print(f\"Original numbers: {numbers}\")\n",
    "print(f\"Squared: {squared_numbers}\")\n",
    "print(f\"Absolute: {absolute_numbers}\")\n",
    "print(f\"\\nOriginal texts: {texts}\")\n",
    "print(f\"Uppercase: {upper_texts}\")\n",
    "\n",
    "# Check statistics\n",
    "print(f\"\\nSquare processor stats: {square_processor.get_stats()}\")\n",
    "print(f\"Text processor stats: {upper_processor.get_stats()}\")"
   ]
  }